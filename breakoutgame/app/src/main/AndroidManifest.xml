<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yogeshdarji.breakout" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.yogeshdarji.breakout.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.yogeshdarji.breakout.scoreactivity"
            android:label="@string/title_activity_scoreactivity" >
        </activity>
        <activity
            android:name="com.example.yogeshdarji.breakout.highscoredisplay"
            android:label="@string/title_activity_highscoredisplay" >
        </activity>
        <activity
            android:name="com.example.yogeshdarji.breakout.highscoresave"
            android:label="@string/title_activity_highscoresave" >
        </activity>
    </application>

</manifest>
